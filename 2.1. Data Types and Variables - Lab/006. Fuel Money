function solve(distance, numPassengers, priceForOneLiterDiesel) {

let neededFuel = (distance / 100) * 7; 
neededFuel += numPassengers * 0.1;
// neededFuel = neededFuel + numPassengers * 0.1; - another option
let money = neededFuel * priceForOneLiterDiesel;

console.log(`Needed money for that trip is ${money} lv.`);

}
